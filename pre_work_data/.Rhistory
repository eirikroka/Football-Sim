library(tidyverse)
club_data <- read.csv("data/data1.csv") %>%
select(Club, Country, Elo, Level) %>%
filter(Level == 1 | Level == 0) %>%
mutate(Group = Country) %>%
select(-Country, -Level) %>%
group_by(Group) %>%
mutate(count = n()) %>%
filter(count > 4) %>%
ungroup() %>%
select(-count)
